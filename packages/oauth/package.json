{
  "name": "@lightsparkdev/oauth",
  "version": "0.1.18",
  "description": "Lightspark JS Oauth SDK",
  "author": "Lightspark Inc.",
  "keywords": [
    "lightspark",
    "bitcoin",
    "lightning",
    "payments",
    "typescript"
  ],
  "homepage": "https://github.com/lightsparkdev/js-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/lightsparkdev/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/lightsparkdev/js-sdk/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14.16"
  },
  "browser": {
    "crypto": false
  },
  "files": [
    "src/*",
    "dist/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf .turbo && rm -rf dist",
    "dev": "yarn build -- --watch",
    "format:fix": "prettier src --write",
    "format": "prettier src --check",
    "lint:fix": "eslint --fix .",
    "lint:watch": "esw ./src -w --ext .ts,.tsx,.js --color",
    "lint": "eslint .",
    "postversion": "yarn build",
    "test": "echo \"TODO\"",
    "types:watch": "tsc-absolute --watch",
    "types": "tsc"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lightsparkdev/core": "0.3.10",
    "@openid/appauth": "^1.3.1",
    "auto-bind": "^5.0.1",
    "dayjs": "^1.11.7"
  },
  "devDependencies": {
    "@lightsparkdev/eslint-config": "*",
    "@lightsparkdev/tsconfig": "0.0.0",
    "@types/text-encoding": "^0.0.36",
    "eslint": "^8.3.0",
    "eslint-watch": "^8.0.0",
    "jest": "^29.4.1",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-jest": "^29.0.5",
    "tsc-absolute": "^1.0.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "private": true
}
