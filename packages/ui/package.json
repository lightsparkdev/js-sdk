{
  "name": "@lightsparkdev/ui",
  "version": "1.0.14",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./components": "./dist/components/index.js",
    "./components/*": "./dist/components/*.js",
    "./components/": "./dist/components/",
    "./components/typography": "./dist/components/typography/index.js",
    "./hooks/*": "./dist/hooks/*.js",
    "./hooks": "./dist/hooks/index.js",
    "./icons": "./dist/icons/index.js",
    "./icons/*": "./dist/icons/*.js",
    "./styles/*": "./dist/styles/*.js",
    "./styles/fonts.css": "./dist/styles/fonts.css",
    "./types/*": "./dist/types/*.js",
    "./types": "./dist/types/index.js",
    "./utils/*": "./dist/utils/*.js",
    "./utils": "./dist/utils/index.js",
    "./static/": "./dist/static/",
    "./router": "./dist/router.js",
    "./src/components": "./src/components/index.js",
    "./src/components/*": "./src/components/*.js",
    "./src/components/typography": "./src/components/typography/index.js",
    "./src/hooks/*": "./src/hooks/*.js",
    "./src/hooks": "./src/hooks/index.js",
    "./src/icons": "./src/icons/index.js",
    "./src/icons/*": "./src/icons/*.js",
    "./src/styles/*": "./src/styles/*.js",
    "./src/styles/fonts.css": "./src/styles/fonts.css",
    "./src/types/*": "./src/types/*.js",
    "./src/types": "./src/types/index.js",
    "./src/utils/*": "./src/utils/*.js",
    "./src/utils": "./src/utils/index.js",
    "./src/static/": "./src/static/",
    "./src/router": "./src/router.js"
  },
  "scripts": {
    "build-tsup": "tsup --dts",
    "build": "yarn tsc && yarn build-tsup",
    "build:watch": "yarn build-tsup --watch --clean=false",
    "circular-deps": "madge --circular --extensions ts,tsx .",
    "clean": "rm -rf .turbo",
    "format:fix": "prettier . --write",
    "format": "prettier . --check",
    "lint:fix": "eslint --fix .",
    "lint:watch": "esw . -w --ext .ts,.tsx,.js --color",
    "lint": "eslint .",
    "test": "echo \"ui package tests are located in apps/examples/ui-test-app\"",
    "types": "yarn tsc",
    "types:watch": "tsc-absolute --watch"
  },
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@lightsparkdev/core": "1.2.6",
    "@rollup/plugin-url": "^8.0.2",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@svgr/core": "^8.1.0",
    "@wojtekmaj/react-daterange-picker": "^5.5.0",
    "@wojtekmaj/react-datetimerange-picker": "^5.5.0",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.19.2",
    "deep-object-diff": "^1.1.9",
    "deepmerge": "^4.3.1",
    "libphonenumber-js": "^1.11.1",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "next": "^13.4.19",
    "prismjs": "^1.29.0",
    "qrcode.react": "^4.0.1",
    "react": "^18.2.0",
    "react-date-picker": "^10.6.0",
    "react-datetime-picker": "^5.6.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.1.0",
    "react-router-dom": "6.11.2",
    "react-select": "^5.4.0",
    "react-table": "^7.8.0",
    "react-tooltip": "^5.10.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@lightsparkdev/eslint-config": "*",
    "@lightsparkdev/tsconfig": "0.0.1",
    "@microsoft/api-extractor": "^7.47.9",
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/prismjs": "^1.26.0",
    "@types/react-table": "^7.7.18",
    "esbuild": "0.19.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-svgr": "^2.1.0",
    "eslint": "^8.3.0",
    "eslint-watch": "^8.0.0",
    "graphql": "^16.6.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.4",
    "madge": "^6.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-jest": "^29.1.1",
    "tsc-absolute": "^1.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true,
        "skipAsyncImports": true
      },
      "tsx": {
        "skipTypeImports": true,
        "skipAsyncImports": true
      }
    }
  },
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18"
  }
}
