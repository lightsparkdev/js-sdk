{
  "name": "@lightsparkdev/ui",
  "version": "0.0.7",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./components": "./dist/components/index.js",
    "./components/*": "./dist/components/*.js",
    "./components/": "./dist/components/",
    "./hooks/*": "./dist/hooks/*.js",
    "./hooks": "./dist/hooks/index.js",
    "./icons": "./dist/icons/index.js",
    "./icons/*": "./dist/icons/*.js",
    "./styles/*": "./dist/styles/*.js",
    "./styles/fonts.css": "./dist/styles/fonts.css",
    "./types/*": "./dist/types/*.js",
    "./types": "./dist/types/index.js",
    "./utils/*": "./dist/utils/*.js",
    "./utils": "./dist/utils/index.js",
    "./static/": "./dist/static/",
    "./router": "./dist/router.js"
  },
  "scripts": {
    "build": "yarn tsc && tsup --dts",
    "build:watch": "yarn build --watch --clean=false",
    "circular-deps": "madge --circular --extensions ts,tsx .",
    "clean": "rm -rf .turbo",
    "format:fix": "prettier . --write",
    "format": "prettier . --check",
    "lint:fix": "eslint --fix .",
    "lint:watch": "esw . -w --ext .ts,.tsx,.js --color",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules $(yarn bin jest) --no-cache --runInBand --bail",
    "types": "yarn tsc",
    "types:watch": "tsc-absolute --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@lightsparkdev/core": "1.0.16",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "gql": "^1.1.2",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "6.11.2",
    "react-tooltip": "^5.10.1"
  },
  "devDependencies": {
    "@lightsparkdev/eslint-config": "*",
    "@lightsparkdev/tsconfig": "0.0.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "babel-jest": "^29.6.4",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-svgr": "^2.1.0",
    "eslint": "^8.3.0",
    "eslint-watch": "^8.0.0",
    "graphql": "^16.6.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.4",
    "madge": "^6.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-jest": "^29.1.1",
    "tsc-absolute": "^1.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ]
}
