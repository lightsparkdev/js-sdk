{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    },
    "build": {
      "outputs": ["dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "circular-deps": {},
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docs": {
      "outputs": ["docs/**"],
      "dependsOn": ["^build"]
    },
    "format": {},
    "gql-codegen": {},
    "lint": {},
    "test": {},
    "types": {}
  }
}
